<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th width="40%"><%= t("report.subsidiary_workloadings.show.table.user_name") -%></th>
      <th width="30%"><%= t("report.subsidiary_workloadings.show.table.date") -%></th>
      <th width="10%"><%= t("report.subsidiary_workloadings.show.table.blue_print_need") -%></th>
      <th width="10%"><%= t("report.subsidiary_workloadings.show.table.blue_print_real") -%></th>
      <th width="10%"><%= t("report.subsidiary_workloadings.show.table.blue_print_rate") -%></th>
    </tr>
  </thead>
  <tbody>
    <% @data.each do |d| %>
      <tr>
        <td><%= d.user_name -%></td>
        <td><%= d.date.to_date -%></td>
        <td><%= d.blue_print_need -%></td>
        <td><%= d.blue_print_real -%></td>
        <td><%= d.blue_print_rate -%></td>
      </tr>
    <% end %>
  </tbody>
</table>
