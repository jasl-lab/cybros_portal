<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th width="40%"><%= t("report.subsidiary_workloadings.show.table.departmentname") -%></th>
      <th width="30%"><%= t("report.subsidiary_workloadings.show.table.date") -%></th>
      <th width="10%"><%= t("report.subsidiary_workloadings.show.table.date_need") -%></th>
      <th width="10%"><%= t("report.subsidiary_workloadings.show.table.date_real") -%></th>
      <th width="10%"><%= t("report.subsidiary_workloadings.show.table.fill_rate") -%></th>
    </tr>
  </thead>
  <tbody>
    <% @data.each do |d| %>
      <tr>
        <td><%= d.departmentname -%></td>
        <td><%= d.date.to_date -%></td>
        <td><%= d.date_need -%></td>
        <td><%= d.date_real -%></td>
        <td><%= d.fill_rate -%></td>
      </tr>
    <% end %>
  </tbody>
</table>
