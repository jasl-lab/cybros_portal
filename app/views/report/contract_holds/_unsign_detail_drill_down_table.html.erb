<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th width="20%"><%= t("report.contract_holds.show.table.projectitemcode") -%></th>
      <th width="30%"><%= t("report.contract_holds.show.table.contractcode") -%></th>
      <th width="10%"><%= t("report.contract_holds.show.table.profession") -%></th>
      <th width="10%"><%= t("report.contract_holds.show.table.planning_output") -%></th>
      <th width="10%"><%= t("report.contract_holds.show.table.workhour_cost") -%></th>
      <th width="20%"><%= t("report.contract_holds.show.table.unsign_hold_value") -%></th>
    </tr>
  </thead>
  <tbody>
    <% rows.each do |r| %>
      <tr>
        <td><%= r.projectitemcode -%></td>
        <td><%= r.contractcode -%></td>
        <td><%= r.profession -%></td>
        <td><%= r.planning_output -%></td>
        <td><%= r.workhour_cost -%></td>
        <td><%= r.unsign_hold_value -%></td>
      </tr>
    <% end %>
  </tbody>
</table>
