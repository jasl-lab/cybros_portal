<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th width="13%"><%= t("report.subsidiary_complete_values.show.table.projectcode") -%></th>
      <th width="27%"><%= t("report.subsidiary_complete_values.show.table.projectname") -%></th>
      <th width="13%"><%= t("report.subsidiary_complete_values.show.table.contractcode") -%></th>
      <th width="12%"><%= t("report.subsidiary_complete_values.show.table.salescontractname") -%></th>
      <th width="10%"><%= t("report.subsidiary_complete_values.show.table.sumamount") -%></th>
      <th width="11%"><%= t("report.subsidiary_complete_values.show.table.confirmdate") -%></th>
    </tr>
  </thead>
  <tbody>
    <% rows.each do |r| %>
      <tr>
        <td><%= r.projectcode -%></td>
        <td><%= r.projectname -%></td>
        <td><%= r.contractcode -%></td>
        <td><%= r.salescontractname -%></td>
        <td><%= r.sumamount -%></td>
        <td><%= r.confirmdate -%></td>
      </tr>
    <% end %>
  </tbody>
</table>
