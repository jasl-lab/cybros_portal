<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th width="13%"><%= t("report.predict_contracts.show.table.opportunitycode") -%></th>
      <th width="27%"><%= t("report.predict_contracts.show.table.opportunityname") -%></th>
      <th width="13%"><%= t("report.predict_contracts.show.table.contractamount") -%></th>
      <th width="12%"><%= t("report.predict_contracts.show.table.cnname") -%></th>
      <th width="10%"><%= t("report.predict_contracts.show.table.successrate") -%></th>
      <th width="11%"><%= t("report.predict_contracts.show.table.contractconvert") -%></th>
      <th width="14%"><%= t("report.predict_contracts.show.table.businessdirectorname") -%></th>
    </tr>
  </thead>
  <tbody>
    <% rows.each do |r| %>
      <tr>
        <td><%= r.opportunitycode -%></td>
        <td><%= r.opportunityname -%></td>
        <td><%= r.contractamount.round(0) -%></td>
        <td><%= r.cnname -%></td>
        <td><%= (r.successrate * 100).round(0) -%>%</td>
        <td><%= r.contractconvert.round(0) -%></td>
        <td><%= r.businessdirectorname -%></td>
      </tr>
    <% end %>
  </tbody>
</table>
