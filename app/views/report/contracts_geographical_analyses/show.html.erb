<div class="row">
  <div class="col-lg-12"
    data-controller="contract-geographical-analysis"
    data-action="resize@window->contract-geographical-analysis#layout">
    <h3 class="page-title">
      <%= t(".title") %>
    </h3>
    <%= form_tag report_contracts_geographical_analysis_path, method: :get, class: 'form-inline' do -%>
      <div class="form-group">
        <label class="mr-1"><%= t(".year_names") -%>：</label>
        <%= select_tag 'year_names', options_for_select(@all_year_names, @year_names), { multiple: true, class: 'form-control' } %>
      </div>
      <div class="form-group mr-3">
        <label class="mr-1"><%= t(".org_names") -%>：</label>
        <%= select_tag 'orgs', options_for_select(@organization_options, @orgs_options), { include_blank: true, multiple: true, class: 'form-control' } %>
      </div>
      <div><%= submit_tag t(".confirm"), class: 'btn btn-primary' %></div>
    <% end -%>
    <div class="row">
    </div>
  </div>
</div>
