<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th width="30%"><%= t("report.subsidiary_daily_workloading.table.date") -%></th>
      <th width="10%"><%= t("report.subsidiary_daily_workloading.table.construction_need") -%></th>
      <th width="10%"><%= t("report.subsidiary_daily_workloading.table.construction_real") -%></th>
      <th width="10%"><%= t("report.subsidiary_daily_workloading.table.construction_rate") -%></th>
    </tr>
  </thead>
  <tbody>
    <% @data.each do |d| %>
      <tr>
        <td><%= d.date.to_date -%></td>
        <td><%= d.construction_need -%></td>
        <td><%= d.construction_real -%></td>
        <td><%= d.construction_rate && (d.construction_rate.to_f * 100).round(1) -%>%</td>
      </tr>
    <% end %>
  </tbody>
</table>
