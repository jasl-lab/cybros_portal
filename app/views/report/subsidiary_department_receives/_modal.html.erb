<div class="modal-dialog modal-lg" role="document">
  <div class="modal-content">
    <div class="modal-header">
      <h5 class="modal-title"><%= @company_name -%> <%= @department_name -%> 本年业绩合同额明细</h5>
      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
    <div class="modal-body">
      <table class="table table-striped table-bordered">
        <thead>
          <tr>
            <th width="30%"><%= t(".table.projectname") -%></th>
            <th width="10%"><%= t(".table.salescontractcode") -%></th>
            <th width="30%"><%= t(".table.salescontractname") -%></th>
            <th width="15%"><%= t(".table.real_receive") -%></th>
            <th width="15%"><%= t(".table.realdate") -%></th>
          </tr>
        </thead>
        <tbody>
          <% rows.each do |d| %>
            <tr>
              <td><%= d.projectname -%></td>
              <td><%= d.contractcode -%></td>
              <td><%= d.salescontractname -%></td>
              <td><%= d.real_receive -%></td>
              <td><%= d.realdate.to_s(:short_date) -%></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
    <div class="modal-footer">
      <button type="button" class="btn btn-primary" data-dismiss="modal"><%= t('.close') -%></button>
    </div>
  </div>
</div
