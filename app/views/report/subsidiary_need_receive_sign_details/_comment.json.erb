<%# Must end as json as it's called from json %>
<%= form_with model: coc, url: report_comment_on_sales_contract_code_path, method: :post, format: :js, class: 'form-inline' do |f| %>
  <%= f.hidden_field :sales_contract_code %>
  <%= f.hidden_field :record_month %>
  <div class="form-group">
    <label class="sr-only">备注</label>
    <%= f.text_field :comment, class: 'form-control' %>
  </div>
  <div class="form-group">
    <button type="submit" class="btn btn-sm btn-primary" id="<%= "btn-#{coc.sales_contract_code}" -%>">保存</button>
  </div>
<% end %>
