<td><%= s.phone_registrant -%></td>
<td><%= s.receive_id -%></td>
<td><%= s.user&.chinese_name -%></td>
<td><%= s.user_mobile -%></td>
<td>
  <%= time_ago_in_words s.updated_at -%>
  <% if s.charging %>
    <i class="fa fa-charging-station"></i>
  <% else %>
    <i class="fa fa-car-battery"></i>
  <% end %>
  <%= s.battery.to_i -%> %
</td>
<td>
  <%= link_to edit_company_sms_phone_configuration_path(id: s.id), remote: true do %>
    <i class="fa fa-edit"></i>
  <% end %>
</td>
